/* Task 1: Write a expression that cheks if given integer is odd or even */

using System;
class EvenOrOdd
{
    static void Main()
    {
        Console.Title = "EvenOrOdd";
        int number = int.Parse(Console.ReadLine());
        if (number % 2 == 0)
        {
            Console.WriteLine(" The number is even ");
        }
        else
        {
            Console.WriteLine(" The number is odd ");
        }
    }
}
